    "vue": "^3.5.12",
    "vuepress": "2.0.0-rc.18"
  },
  "devDependencies": {
    "tsconfig-vuepress": "^5.2.1",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=18.19.0"
  }
